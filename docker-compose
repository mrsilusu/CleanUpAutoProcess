services:
  web:
    image: nginx:latest
    hostname: ''
    init: false
    ports:
      - '80:80'
    environment: {}
    volumes: []
    depends_on: []
    networks: []
    deploy:
      resources:
        limits:
          cpus: ''
          memory: ''
    restart: always
    stop_grace_period: 1m
    healthcheck:
      test: wget -O /dev/null localhost:80 || exit 1
      timeout: 5s
      retries: 3
networks: {}
